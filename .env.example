# EmployerBot (работодатели)
BOT_TOKEN_EMPLOYER=your_employer_bot_token_from_botfather

# WorkerBot (работники)
BOT_TOKEN_WORKER=your_worker_bot_token_from_botfather

# Database (PostgreSQL)
DATABASE_URL=postgresql://trinity:trinity@localhost:5432/trinity

# Optional: admin gets copy of each published job
ADMIN_TELEGRAM_ID=

# Timezone (default: Asia/Tashkent)
APP_TIMEZONE=Asia/Tashkent

# Broadcast rate limiting
RATE_LIMIT_PER_MINUTE=60
MAX_BROADCAST_PER_JOB=500
MIN_BROADCAST_BATCH_DELAY_MS=300
